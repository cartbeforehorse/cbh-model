{
    "name"        : "cartbeforehorse/cbh-model",
    "description" : "Extending and combining all the best vendor extensions to Eloquent Models",
    "keywords"    : ["laravel", "eloquent", "model", "validation", "dual-pk", "finite-state-machine", "fsm"],
    "type"        : "library",
    "license"     : "MIT",
    "authors": [
        {
            "name": "Osian ap Garth",
            "email": "osian.ap.garth@cbhsoftware.com"
        }
    ],
    "repositories" : [{
        "type" : "vcs",
        "url"  : "https://github.com/cartbeforehorse/cbh-validation"
    }, {
        "type" : "package",
        "package": {
            "name" : "cartbeforehorse/laravel-oci8",
            "version": "dev-master",
            "source" : {
                "url"  : "https://github.com/cartbeforehorse/laravel-oci8.git",
                "type": "git",
                "reference": "master"
            }
        }
    }],
    "require": {
        "php"                             : ">=7.0",
        "watson/validating"               : "^6.1",
        "sebdesign/laravel-state-machine" : "^3.2",
        "cartbeforehorse/cbh-validation"  : ">=0.0.1",
        "cartbeforehorse/laravel-oci8"    : "dev-master",
        "illuminate/support"              : "^8.55",
        "illuminate/database"             : "^8.55"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Cartbeforehorse\\DbModels\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
